<div class="col-md-12 mg-b10">
  <%= form_for([@project, @integration], html: {role: "form", class: "form-horizontal"}) do |f| %>
      <div class="form-group">
        <%= f.label :name, 'Name', class: "col-md-2" %>
        <div class="col-md-10">
          <%= f.url_field :name, class: "form-control" %>
        </div>
      </div>
      <div class="form-group">
        <%= f.label :url, 'URL', class: "col-md-2" %>
        <div class="col-md-10">
          <%= f.url_field :url, class: "form-control" %>
        </div>
      </div>
      <div class="form-group">
        <label class="col-md-2">Active</label>

        <div class="checkbox col-md-10">
          <label><input type="checkbox" value=""></label>
        </div>
      </div>
      <%= f.submit class: "btn btn-success pull-right" %>
  <% end %>
</div>
