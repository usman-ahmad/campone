<% content_for(:tab_nav) do %>
  <%= render 'shared/tab_nav', project: @project, active: 'settings' %>
<% end %>

<%= render 'shared/settings_tab', project: @project, class_name: 'active', active: 'integrations' %>

<div class='col-lg-9 col-md-9 col-sm-9 col-xs-9 custom-tab'>

  <h2 class="text-capitalize"><%= params[:name] %></h2>
  <%= button_to "Add #{params[:name]} Integration", project_integrations_path(name: params[:name]), class: 'btn-lg btn-success' %>

  <h3>Manage existing integrations</h3>

  <div class='col-md-12'>
    <%= render 'integrations/list', project: @project, integrations: @integrations %>
  </div>
</div>

